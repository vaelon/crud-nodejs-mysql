{
  "name": "lightrun",
  "version": "1.69.1",
  "author": "Lightrun platform Inc.",
  "description": "Lightrun Debug Agent for Node.js",
  "license": "SEE LICENSE IN LIGHTRUN",
  "main": "./build/src/index",
  "types": "./build/src/index.d.ts",
  "repository": "lightrun/lightrun",
  "keywords": [
    "lightrun",
    "debugger",
    "debug",
    "cloud",
    "cloud debug",
    "cloud debugger"
  ],
  "engines": {
    "node": ">=16"
  },
  "files": [
    "build/src",
    "start.js",
    "lambda.js",
    "gcp.js"
  ],
  "scripts": {
    "clean": "gts clean",
    "precompile": "gts clean",
    "compile": "tsc -p .",
    "postcompile": "shx cp -r src/resources build/src && shx mkdir -p build/test/misc && shx cp -r test/*.js build/test && shx cp -r test/misc/*.js build/test/misc && shx cp -r test/fixtures build/test",
    "build": "npm run compile",
    "presystem-test": "npm run compile-test",
    "system-test": "nyc mocha build/system-test --timeout 600000",
    "test": "npm run prettier:check && npm run lint && nyc cross-env CLOUD_DEBUG_ASSERTIONS=1 mocha --exit -p build/test --retries 2",
    "test-all": "npm run build && npm run test && npm run system-test",
    "compile-test": "tsc -p tsconfig.test.json",
    "fix": "gts fix",
    "pretest": "npm run compile-test",
    "prepack": "npm run obfuscate && npm run api-extractor && shx rm -r \"build/src/**/*.d.ts\" && shx cp build/types/index.d.ts build/src/index.d.ts",
    "lint": "gts check",
    "api-extractor": "api-extractor run --local",
    "obfuscate": "javascript-obfuscator build -o build/ --config obfuscating-options.json",
    "prettier:format": "prettier --write \"./{src,test,system-test}/**/*.ts\"",
    "prettier:check": "prettier --list-different \"./{src,test,system-test}/**/*.ts\""
  },
  "dependencies": {
    "@google-cloud/common": "^3.10.0",
    "acorn": "^8.14.0",
    "axios": "^1.12.0",
    "extend": "^3.0.2",
    "findit2": "^2.2.3",
    "https-proxy-agent": "5.0.1",
    "java-properties": "^1.0.2",
    "js-yaml": "^4.1.0",
    "memory-streams": "^0.1.3",
    "p-limit": "^3.1.0",
    "re2js": "0.4.3",
    "semver": "^7.6.3",
    "source-map": "^0.6.1",
    "split": "^1.0.1",
    "triple-beam": "1.4.1",
    "winston": "3.5.0",
    "winston-daily-rotate-file": "^4.7.1",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.16.11",
    "@microsoft/api-extractor": "7.38.5",
    "@types/acorn": "^4.0.6",
    "@types/estree": "^1.0.6",
    "@types/events": "^3.0.3",
    "@types/extend": "^3.0.4",
    "@types/mocha": "^8.0.0",
    "@types/mv": "^2.1.4",
    "@types/ncp": "^2.0.8",
    "@types/node": "^16.11.7",
    "@types/proxyquire": "^1.3.31",
    "@types/semver": "^7.5.8",
    "@types/sinon": "^10.0.11",
    "@types/tmp": "^0.2.6",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "execa": "^4.0.0",
    "gts": "^4.0.1",
    "javascript-obfuscator": "4.1.1",
    "mocha": "9.2.2",
    "mocha-sinon": "^2.1.2",
    "mv": "^2.1.1",
    "ncp": "^2.0.0",
    "nock": "^13.5.6",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "send": "0.19.0",
    "shx": "^0.3.4",
    "sinon": "^13.0.2",
    "teeny-request": "^7.0.0",
    "timekeeper": "^2.3.1",
    "tmp-promise": "^3.0.3",
    "typescript": "5.2.2",
    "uuid": "^11.0.3"
  }
}
